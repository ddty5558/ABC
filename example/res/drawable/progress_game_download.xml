<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <stroke android:width="1.0dip" android:color="#ff4ba651" />
            <corners android:radius="3.0dip" />
            <gradient android:startColor="@color/white" android:endColor="@color/white" android:angle="270.0" android:centerY="0.35" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <stroke android:width="1.0dip" android:color="#ff4ba651" />
                <corners android:radius="3.0dip" />
                <gradient android:startColor="#ff85b0e9" android:endColor="#ff85b0e9" android:angle="270.0" android:centerY="0.35" />
            </shape>
        </clip>
    </item>
</layer-list>