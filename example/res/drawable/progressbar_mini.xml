<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ffeeeeee" android:endColor="#ffeeeeee" android:angle="270.0" android:centerY="0.35" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ff85b0e9" android:endColor="#ff165cbc" android:angle="270.0" android:centerY="0.35" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ffff9c00" android:endColor="#ffff9c00" android:angle="270.0" android:centerY="0.35" />
            </shape>
        </clip>
    </item>
</layer-list>